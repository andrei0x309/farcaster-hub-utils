{
  "name": "farcaster-hub-utils",
  "version": "0.1.2",
  "description": "Class to use a signer key for doing hub actions.",
  "repository": "https://github.com/andrei0x309/farcaster-hub-utils",
  "author": "Andrei O",
  "license": "MIT",
  "private": false,
  "keywords": [
    "farcaster",
    "web3",
    "signerkey",
    "farcaster hubs",
    "farcaster utils",
    "farcaster hub class"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/farcaster-hub-utils.umd.js",
  "module": "./dist/farcaster-hub-utils.es.js",
  "exports": {
    ".": {
      "import": "./dist/farcaster-hub-utils.es.js",
      "require": "./dist/farcaster-hub-utils.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint --ext .vue --ignore-path .gitignore --fix src",
    "dev": "vite dev",
    "preversion": "yarn build",
    "release": "yarn preversion && yarn config set version-tag-prefix farcaster-hub-utils@v &&  yarn config set version-git-message farcaster-hub-utils@v%s' && yarn version --patch",
    "postversion": "git push && git push --tags && npm publish",
    "build": "vite build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.10",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^9.6.0",
    "eslint-plugin-vue": "^9.27.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "yarn-upgrade-all": "^0.7.2"
  },
  "dependencies": {
    "@farcaster/hub-nodejs": "^0.11.19"
  }
}
